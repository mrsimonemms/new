{
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 100
  {%- if cookiecutter.type == "svelte" %},
  "plugins": ["prettier-plugin-svelte"],
  "pluginSearchDirs": ["."],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ]
  {%- endif %}
}
